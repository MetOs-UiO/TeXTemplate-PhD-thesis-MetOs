@article{makkonen_air_2012,
	title = {Air pollution control and decreasing new particle formation lead to strong climate warming},
	volume = {12},
	issn = {1680-7316},
	url = {https://www.atmos-chem-phys.net/12/1515/2012/acp-12-1515-2012.html},
	doi = {https://doi.org/10.5194/acp-12-1515-2012},
	abstract = {{\textless}p{\textgreater}{\textless}strong{\textgreater}Abstract.{\textless}/strong{\textgreater} The number concentration of cloud droplets determines several climatically relevant cloud properties. A major cause for the high uncertainty in the indirect aerosol forcing is the availability of cloud condensation nuclei (CCN), which in turn is highly sensitive to atmospheric new particle formation. Here we present the effect of new particle formation on anthropogenic aerosol forcing in present-day (year 2000) and future (year 2100) conditions. The present-day total aerosol forcing is increased from −1.0 W m$^{\textrm{−2}}$ to −1.6 W m$^{\textrm{−2}}$ when nucleation is introduced into the model. Nucleation doubles the change in aerosol forcing between years 2000 and 2100, from +0.6 W m$^{\textrm{−2}}$ to +1.4 W m$^{\textrm{−2}}$. Two climate feedbacks are studied, resulting in additional negative forcings of −0.1 W m$^{\textrm{−2}}$ (+10\% DMS emissions in year 2100) and −0.5 W m$^{\textrm{−2}}$ (+50\% BVOC emissions in year 2100). With the total aerosol forcing diminishing in response to air pollution control measures taking effect, warming from increased greenhouse gas concentrations can potentially increase at a very rapid rate.{\textless}/p{\textgreater}},
	language = {English},
	number = {3},
	urldate = {2018-08-15},
	journal = {Atmospheric Chemistry and Physics},
	author = {Makkonen, R. and Asmi, A. and Kerminen, V.-M. and Boy, M. and Arneth, A. and Hari, P. and Kulmala, M.},
	month = feb,
	year = {2012},
	pages = {1515--1524}
}

