	author = {Bentsen, M. and Bethke, I. and Debernard, J. B. and Iversen, T. and Kirkevåg, A. and Seland, Ø and Drange, H. and Roelandt, C. and Seierstad, I. A. and Hoose, C. and Kristjánsson, J. E.},
