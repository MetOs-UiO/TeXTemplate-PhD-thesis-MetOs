@article{tunved_high_2006,
	title = {High {Natural} {Aerosol} {Loading} over {Boreal} {Forests}},
	volume = {312},
	issn = {0036-8075},
	url = {http://science.sciencemag.org/content/312/5771/261},
	doi = {10.1126/science.1123052},
	abstract = {Aerosols play a key role in the radiation balance of the atmosphere. Here, we present evidence that the European boreal region is a substantial source of both aerosol mass and aerosol number. The investigation supplies a straightforward relation between emissions of monoterpenes and gas-to-particle formation over regions substantially lacking in anthropogenic aerosol sources. Our results show that the forest provides an aerosol population of 1000 to 2000 particles of climatically active sizes per cubic centimeter during the late spring to early fall period. This has important implications for radiation budget estimates and relevancy for the evaluation of feedback loops believed to determine our future climate.},
