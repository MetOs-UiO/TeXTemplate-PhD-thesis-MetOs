@article{hakkinen_semi-empirical_2013,
	title = {Semi-empirical parameterization of size-dependent atmospheric nanoparticle growth in continental environments},
	volume = {13},
	issn = {1680-7316},
	url = {https://www.atmos-chem-phys.net/13/7665/2013/},
